package Classes;

public class DataTeste {

	
	public static void main(String[] args) {
		
		Data data1 = new Data();
		Data data2 = new Data();
		
		data1.ano = 2022;
		data1.mes = 3;
		data1.dia = 23;
		
		data2.ano = 1986;
		data2.mes = 12;
		data2.dia = 24;
		
		System.out.printf("1° Data: %d de %s de %s! \n", data1.dia, data1.teste(data1.mes), data1.ano);
		System.out.printf("2° Data: %d de %s de %s!", data2.dia, data2.teste(data2.mes), data2.ano);
	}
}
