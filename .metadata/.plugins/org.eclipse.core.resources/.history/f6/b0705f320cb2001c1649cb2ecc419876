package oo.Composicao;

import java.util.ArrayList;

public class Compra {

	String cliente;
	double valor;
	
	ArrayList<Item> itens = new ArrayList<Item>();
	
	void obterValor() {
		
		for( Item valores: itens) {
			valor += (valores.preco * valores.quantidade);
			}
		System.out.println(valor);
	
	}
}

