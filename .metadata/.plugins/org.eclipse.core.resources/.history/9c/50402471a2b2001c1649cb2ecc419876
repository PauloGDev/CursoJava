package oo.Composicao;

import java.util.ArrayList;
import java.util.List;

public class Curso {

	final String nome;
	
	final ArrayList<Aluno> alunos = new ArrayList<>();
	
	void addAluno(Aluno aluno){
		this.alunos.add(aluno);
		aluno.cursos.add(this);
	}

	Curso(String nome){
		this.nome = nome;
	}
}
