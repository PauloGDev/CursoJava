package Arrays;

public class AulaArrays {

	public static void main(String[] args) {
		double[] a = new double[4];
		a[0] = 8;
		a[1] = 7;
		a[2] = 2;
		a[3] = 9;
		double media = 0;
		
		for(int c = 0; c < 4; c++) {
			media += a[c];
		}
		System.out.printf("A media dos numeros é: %.2f", media/4);
	}
}
