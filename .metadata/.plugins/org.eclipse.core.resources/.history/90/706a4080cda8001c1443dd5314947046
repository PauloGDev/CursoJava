package Fundamentos;

public class ForDesafio2 {

	public static void main(String[] args) {
		
		for(String v = "#"; v != "#####"; v += "#") {
			if (v != "#####") {
				v+= "#";
				System.out.println(v);
			}
			else if(v == "#####") {
				break;
			}
			
		}
		
		//for(int i = 1; i != 10; i +=1) {
			//System.out.println(i);
		//}
	}
}
